@misc{Wilbur1992,
abstract = {A stop word may be identified as a word that has the same likehhood of occurring in those documents not relevant to a query as in those documents relevant to the query. In this paper we show how the concept of relevance may be replaced by the condition of being highly rated by a similarity measure. Thus it becomes possible to identify the stop words in a cullectmn by automated statistical testing. We describe the nature of the statistical test as it is realized with a vector retrieval methodology based on the cosine coefficient of document-document similarity. As an example, this tech nique is then applied to a large MEDLINE " subset in the area of biotechnology. The initial processing of this datahase involves a 310 word stop list of common non-content terms. Our technique is then applied and 75\% of the remaining terms are identified as stop words. We compare retrieval with and without the removal of these stop words and find that of the top twenty documents retrieved in response to a random query document. seventeen of these are the same on the average for the two methods We also examine the differences and conclude that where the user prefers one method over the other, the new method with the reduced term set is favored about three times out of four.},
author = {Wilbur, W. J. and Sirotkin, K.},
booktitle = {Journal of Information Science},
doi = {10.1177/016555159201800106},
isbn = {0165-5515},
issn = {0165-5515},
pages = {45--55},
title = {{The automatic identification of stop words}},
volume = {18},
year = {1992}
}
@article{Shani2005,
abstract = {Typical recommender systems adopt a static view of the recommendation$\backslash$nprocess and treat it as a prediction problem. We argue that it is$\backslash$nmore appropriate to view the problem of generating recommendations$\backslash$nas a sequential optimization problem and, consequently, that Markov$\backslash$ndecision processes (MDPs) provide a more appropriate model for recommender$\backslash$nsystems. MDPs introduce two benefits: they take into account the$\backslash$nlong-term effects of each recommendation and the expected value of$\backslash$neach recommendation. To succeed in practice, an MDP-based recommender$\backslash$nsystem must employ a strong initial model, must be solvable quickly,$\backslash$nand should not consume too much memory. In this paper, we describe$\backslash$nour particular MDP model, its initialization using a predictive model,$\backslash$nthe solution and update algorithm, and its actual performance on$\backslash$na commercial site. We also describe the particular predictive model$\backslash$nwe used which outperforms previous models. Our system is one of a$\backslash$nsmall number of commercially deployed recommender systems. As far$\backslash$nas we know, it is the first to report experimental analysis conducted$\backslash$non a real commercial site. These results validate the commercial$\backslash$nvalue of recommender systems, and in particular, of our MDP-based$\backslash$napproach.},
archivePrefix = {arXiv},
arxivId = {1301.0600},
author = {Shani, Guy and Heckerman, David and Brafman, Ronen I},
eprint = {1301.0600},
file = {:Users/hcorona/Google Drive/papers/2005/J. Mach. Learn. Res/2005 - Shani, Heckerman, Brafman - An MDP-Based Recommender System.pdf:pdf},
isbn = {1533-7928},
issn = {1532-4435},
journal = {J. Mach. Learn. Res.},
keywords = {commercial applications,learning,markov decision processes,recommender systems},
pages = {1265--1295},
title = {{An MDP-Based Recommender System}},
url = {http://portal.acm.org/citation.cfm?id=1046920.1088715},
volume = {6},
year = {2005}
}

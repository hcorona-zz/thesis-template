doi = {10.1109/ICIS.2010.142},
file = {:Users/hcorona/Google Drive/papers/2010/Computer and Information Science (ICIS), 2010 IEEEACIS 9th International Conference on/2010 - Park, Chan Ho - Temporal Dynamics in Music Listening Behavior A Case Study of Online Music Service.pdf:pdf},
isbn = {978-1-4244-8198-9},
journal = {Computer and Information Science (ICIS), 2010 IEEE/ACIS 9th International Conference on},
keywords = {context-awareness,data analysis,online music,popularity,recommendation,temporal dynamics,usage log analysis},
